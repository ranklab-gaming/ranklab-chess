{
  "extends": ["config:base"],
  "automerge": true,
  "labels": ["dependencies"],
  "prHourlyLimit": 10,
  "timezone": "UTC",
  "platform": "github",
  "repositories": [
    {
      "matchDatasource": "github",
      "datasourceTemplate": "${datasource}:${owner}/${repository}",
      "depName": null,
      "schedule": ["before 9am on Monday"],
      "regex": "^ranklab-chess$"
    }
  ]
}
